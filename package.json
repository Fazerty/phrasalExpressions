    "test": "NODE_ENV='test' TS_NODE_PROJECT='./tsconfig.json' mocha --file ./tests/init.ts --require ts-node/register --recursive ./tests/**/*.tests.ts --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fazerty/phrasalExpressions.git"
