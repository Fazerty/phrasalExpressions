  "bugs": {
    "url": "https://github.com/Fazerty/phrasalExpressions/issues"
  },
  "homepage": "https://github.com/Fazerty/phrasalExpressions#readme",
  "dependencies": {
    "regexp-tree": "^0.0.86"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2",
    "chai": "^4.2.0",
    "mocha": "^5.2.0"
  }
